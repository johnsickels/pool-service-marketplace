<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="pool_model_action" model="ir.actions.act_window">
        <field name="name">Accounts</field>
        <field name="res_model">pool_broker.pool</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="pool_tree_view" model="ir.ui.view">
        <field name="name">pool_broker.pool.tree</field>
        <field name="model">pool_broker.pool</field>
        <field name="arch" type="xml">
            <tree>
                <field name="location" />
                <field name="current_service_provider" />
            </tree>
        </field>
    </record>

</odoo>